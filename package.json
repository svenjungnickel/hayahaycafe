{
  "name": "hayahaycafe",
  "private": true,
  "description": "www.hayahaycafe.com based on gatsby",
  "author": "Sven Jungnickel <svenjungnickel@googlemail.com>",
  "homepage": "https://www.hayahaycafe.com",
  "engines": {
    "node": ">=16"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "fslightbox-react": "^1.6.2-2",
    "gatsby": "^4.17.2",
    "gatsby-plugin-catch-links": "^4.17.0",
    "gatsby-plugin-google-analytics": "^4.17.0",
    "gatsby-plugin-image": "^2.17.0",
    "gatsby-plugin-manifest": "^4.17.0",
    "gatsby-plugin-netlify": "^5.0.0",
    "gatsby-plugin-netlify-cms": "^6.17.0",
    "gatsby-plugin-offline": "^5.17.0",
    "gatsby-plugin-postcss": "^5.18.0",
    "gatsby-plugin-react-helmet": "^5.18.0",
    "gatsby-plugin-sass": "^5.17.0",
    "gatsby-plugin-sharp": "^4.17.0",
    "gatsby-plugin-use-dark-mode": "^1.5.0",
    "gatsby-remark-copy-linked-files": "^5.18.0",
    "gatsby-remark-images": "^6.18.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^4.17.0",
    "gatsby-transformer-remark": "^5.17.0",
    "gatsby-transformer-sharp": "^4.17.0",
    "gatsby-transformer-yaml": "^4.17.0",
    "lodash": "^4.17.20",
    "netlify-cms-app": "^2.15.72",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "postcss": "^8.4.14",
    "prop-types": "^15.8.1",
    "react": "^17.0.2",
    "react-bootstrap": "^2.4.0",
    "react-dom": "^17.0.2",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^6.1.0",
    "react-icons": "^4.4.0",
    "react-reveal": "^1.2.2",
    "react-switch": "^7.0.0",
    "sass": "^1.53.0",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.18.2",
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/jest-runner": "^6.1.2",
    "@testing-library/cypress": "^8.0.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.7",
    "axe-core": "^4.4.2",
    "babel-jest": "^28.1.2",
    "babel-loader": "^8.2.5",
    "babel-preset-gatsby": "^2.17.0",
    "cypress": "^10.3.0",
    "cypress-audit": "^1.1.0",
    "cypress-axe": "^1.0.0",
    "cypress-intellij-reporter": "^0.0.7",
    "enzyme": "^3.11.0",
    "eslint": "^8.18.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^28.1.2",
    "jest-environment-jsdom": "^28.1.2",
    "netlify-cli": "^10.7.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "^17.0.2",
    "start-server-and-test": "^1.11.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build --log-pages",
    "netlifycms:proxyserver": "npx netlify-cms-proxy-server",
    "develop": "gatsby develop",
    "clean:develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "test": "jest",
    "lint": "eslint ./src/ && echo 'Lint complete.'",
    "lint:fix": "eslint --fix ./src/ && echo 'Lint fix complete.'",
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "cypress:run:record": "cypress run --record",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cypress:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cypress:run",
    "test:e2e:ci:record": "start-server-and-test develop http://localhost:8000 cypress:run:record",
    "lighthouse": "cypress open --config-file cypress-lighthouse.config.js",
    "lighthouse:ci": "cypress run --config-file cypress-lighthouse.config.js",
    "lighthouse:ci:record": "cypress run --record --config-file cypress-lighthouse.config.js",
    "stryker:run": "stryker run"
  }
}
