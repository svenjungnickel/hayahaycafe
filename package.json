{
  "name": "hayahaycafe",
  "private": true,
  "description": "www.hayahaycafe.com based on gatsby",
  "author": "Sven Jungnickel <svenjungnickel@googlemail.com>",
  "engines": {
    "node": ">=14"
  },
  "dependencies": {
    "bootstrap": "^4.5.3",
    "fslightbox-react": "^1.6.0",
    "gatsby": "^2.27.5",
    "gatsby-image": "^2.6.0",
    "gatsby-plugin-catch-links": "^2.5.0",
    "gatsby-plugin-google-analytics": "^2.6.0",
    "gatsby-plugin-manifest": "^2.7.0",
    "gatsby-plugin-netlify": "^2.6.0",
    "gatsby-plugin-netlify-cms": "^4.5.0",
    "gatsby-plugin-offline": "^3.5.0",
    "gatsby-plugin-react-helmet": "^3.5.0",
    "gatsby-plugin-sass": "^2.6.0",
    "gatsby-plugin-sharp": "^2.9.1",
    "gatsby-plugin-use-dark-mode": "^1.2.0",
    "gatsby-remark-copy-linked-files": "^2.5.0",
    "gatsby-remark-images": "^3.6.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.6.1",
    "gatsby-transformer-remark": "^2.11.1",
    "gatsby-transformer-sharp": "^2.7.0",
    "gatsby-transformer-yaml": "^2.6.0",
    "lodash": "^4.17.20",
    "netlify-cms-app": "^2.15.63",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.8.1",
    "react": "^16.14.0",
    "react-bootstrap": "^1.4.0",
    "react-dom": "^16.14.0",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.11.0",
    "react-reveal": "^1.2.2",
    "react-switch": "^5.0.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^4.1.2",
    "@stryker-mutator/jest-runner": "^4.1.2",
    "@testing-library/cypress": "^8.0.2",
    "axe-core": "^4.1.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-preset-gatsby": "^0.7.0",
    "cypress": "^9.2.0",
    "cypress-audit": "^1.1.0",
    "cypress-axe": "^0.14.0",
    "cypress-intellij-reporter": "^0.0.7",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-jest": "^25.3.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^2.5.1",
    "react-test-renderer": "^16.14.0",
    "start-server-and-test": "^1.11.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "netlifycms:proxyserver": "npx netlify-cms-proxy-server",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "test": "jest",
    "lint": "eslint ./src/ && echo 'Lint complete.'",
    "lint:fix": "eslint --fix ./src/ && echo 'Lint fix complete.'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start http://localhost:8000 cy:run",
    "test:e2e:ci:record": "start-server-and-test start http://localhost:8000 cy:run:record",
    "lighthouse": "cypress open --config-file cypress-lighthouse.json",
    "lighthouse:ci": "cypress run --config-file cypress-lighthouse.json",
    "lighthouse:ci:record": "cypress run --record --config-file cypress-lighthouse.json",
    "stryker:run": "stryker run"
  },
  "homepage": "https://www.hayahaycafe.com"
}
