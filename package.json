{
  "name": "hayahaycafe",
  "private": true,
  "description": "www.hayahaycafe.com based on gatsby",
  "version": "0.1.0",
  "author": "Sven Jungnickel <svenjungnickel@googlemail.com>",
  "dependencies": {
    "bootstrap": "^4.3.1",
    "fslightbox-react": "^1.4.9",
    "gatsby": "^2.20.18",
    "gatsby-image": "^2.3.2",
    "gatsby-plugin-google-analytics": "^2.2.2",
    "gatsby-plugin-manifest": "^2.3.3",
    "gatsby-plugin-netlify": "^2.2.1",
    "gatsby-plugin-netlify-cms": "^4.2.2",
    "gatsby-plugin-offline": "^3.1.2",
    "gatsby-plugin-react-helmet": "^3.2.2",
    "gatsby-plugin-sass": "^2.2.1",
    "gatsby-plugin-sharp": "^2.5.4",
    "gatsby-plugin-use-dark-mode": "^1.1.2",
    "gatsby-remark-copy-linked-files": "^2.2.1",
    "gatsby-remark-images": "^3.2.2",
    "gatsby-remark-relative-images": "^0.3.0",
    "gatsby-source-filesystem": "^2.2.2",
    "gatsby-transformer-remark": "^2.7.1",
    "gatsby-transformer-sharp": "^2.4.4",
    "gatsby-transformer-yaml": "^2.3.1",
    "lodash": "^4.17.15",
    "netlify-cms-app": "^2.12.7",
    "netlify-cms-media-library-cloudinary": "^1.3.4",
    "node-sass": "^4.13.1",
    "nuka-carousel": "^4.6.6",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.0.0",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.0.1",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.9.0",
    "react-reveal": "^1.2.2",
    "react-switch": "^5.0.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "axe-core": "^3.5.3",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.3.1",
    "cypress": "^4.3.0",
    "cypress-audit": "^0.0.5",
    "cypress-axe": "^0.8.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.10.1",
    "eslint-loader": "^4.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "gh-pages": "^2.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^25.3.0",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.10.11"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "test": "jest",
    "lint": "eslint ./src/ && echo 'Lint complete.'",
    "lint:fix": "eslint --fix ./src/ && echo 'Lint fix complete.'",
    "deploy": "gatsby build --prefix-paths && gh-pages -d public",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start http://localhost:8000 cy:run",
    "test:e2e:ci:record": "start-server-and-test start http://localhost:8000 cy:run:record",
    "lighthouse": "cypress open --config-file cypress-lighthouse.json",
    "lighthouse:ci": "cypress run --config-file cypress-lighthouse.json",
    "lighthouse:ci:record": "cypress run --record --config-file cypress-lighthouse.json"
  },
  "homepage": "https://www.hayahaycafe.com"
}
