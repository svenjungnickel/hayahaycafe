{
  "name": "hayahaycafe",
  "private": true,
  "description": "www.hayahaycafe.com based on gatsby",
  "author": "Sven Jungnickel <svenjungnickel@googlemail.com>",
  "dependencies": {
    "bootstrap": "^4.5.2",
    "fslightbox-react": "^1.5.0",
    "gatsby": "^2.24.55",
    "gatsby-image": "^2.4.17",
    "gatsby-plugin-catch-links": "^2.3.11",
    "gatsby-plugin-google-analytics": "^2.3.13",
    "gatsby-plugin-manifest": "^2.4.28",
    "gatsby-plugin-netlify": "^2.3.14",
    "gatsby-plugin-netlify-cms": "^4.3.13",
    "gatsby-plugin-offline": "^3.2.27",
    "gatsby-plugin-react-helmet": "^3.3.10",
    "gatsby-plugin-sass": "^2.3.12",
    "gatsby-plugin-sharp": "^2.6.30",
    "gatsby-plugin-use-dark-mode": "^1.1.2",
    "gatsby-remark-copy-linked-files": "^2.3.14",
    "gatsby-remark-images": "^3.3.29",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^2.3.28",
    "gatsby-transformer-remark": "^2.8.34",
    "gatsby-transformer-sharp": "^2.5.14",
    "gatsby-transformer-yaml": "^2.4.11",
    "lodash": "^4.17.20",
    "netlify-cms-app": "^2.12.20",
    "netlify-plugin-gatsby-cache": "^0.3.2",
    "node-sass": "^4.14.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-bootstrap": "^1.3.0",
    "react-dom": "^16.13.1",
    "react-google-recaptcha": "^2.1.0",
    "react-helmet": "^5.2.1",
    "react-icons": "^3.11.0",
    "react-reveal": "^1.2.2",
    "react-switch": "^5.0.1",
    "use-dark-mode": "^2.3.1"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.0",
    "axe-core": "^4.0.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "babel-loader": "^8.1.0",
    "babel-preset-gatsby": "^0.5.9",
    "cypress": "^5.1.0",
    "cypress-audit": "^0.2.2",
    "cypress-axe": "^0.8.1",
    "cypress-intellij-reporter": "0.0.4",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.4",
    "eslint": "^7.8.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.11.1",
    "eslint-plugin-jest": "^24.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.1.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.4.2",
    "jest-prop-type-error": "^1.1.0",
    "prettier": "^2.1.1",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "GATSBY_EXPERIMENTAL_PAGE_BUILD_ON_DATA_CHANGES=true gatsby build --log-pages",
    "develop": "gatsby clean && gatsby develop -H 0.0.0.0 -p 8000",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "gatsby develop",
    "serve": "gatsby clean && gatsby build && gatsby serve",
    "test": "jest",
    "lint": "eslint ./src/ && echo 'Lint complete.'",
    "lint:fix": "eslint --fix ./src/ && echo 'Lint fix complete.'",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "cy:run:record": "cypress run --record",
    "test:e2e": "start-server-and-test start http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test start http://localhost:8000 cy:run",
    "test:e2e:ci:record": "start-server-and-test start http://localhost:8000 cy:run:record",
    "lighthouse": "cypress open --config-file cypress-lighthouse.json",
    "lighthouse:ci": "cypress run --config-file cypress-lighthouse.json",
    "lighthouse:ci:record": "cypress run --record --config-file cypress-lighthouse.json"
  },
  "homepage": "https://www.hayahaycafe.com"
}
