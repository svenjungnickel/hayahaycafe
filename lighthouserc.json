{
  "ci": {
    "collect": {
      "numberOfRuns": 3,
      "url": [
        "https://hayahaycafe.netlify.com/"
      ]
    },
    "assert": {
      "preset": "lighthouse:recommended",
      "assertions": {
        "offscreen-images": [
          "warn",
          {
            "maxNumericValue": 1,
            "aggregationMethod": "optimistic"
          }
        ],
        "unused-css-rules": [
          "warn",
          {
            "maxNumericValue": 1,
            "aggregationMethod": "optimistic"
          }
        ],
        "font-display": [
          "warn",
          {
            "maxNumericValue": 0,
            "aggregationMethod": "optimistic"
          }
        ]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}